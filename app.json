{
  "name": "Telegram RSS Bot",
  "description": "Telegram RSS Bot",
  "keywords": [
    "telegram"
  ],
  "stack": "heroku-20",
  "repository": "https://github.com/kelengkeng/rss",
  "success_url": "https://github.com/kelengkeng/rss",
  "website": "https://github.com/kelengkeng/rss",
  "env": {
    "APP_ID": {
      "description": "Get this value from https://my.telegram.org",
      "value": ""
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "get this value from @BotFather",
      "value": ""
    },
    "FEED_URL": {
      "description": "URL of RSS Feed.",
      "value": ""
    },
    "LOG_CHANNEL": {
      "description": "ID of the Telegram Channel where messages are to be posted.",
      "value": ""
    },
    "ENV": {
      "description": "Just leave it.",
      "value": "True"
    },
    "INTERVAL": {
      "description": "Checking Interval in seconds. (optional)",
      "value": "5"
    },
    "MAX_INSTANCES": {
      "description": "Max instances to be used while checking rss feed. (optional)",
      "value": "5"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "10"
      }
    }
  ]
}
